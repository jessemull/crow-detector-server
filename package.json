{
  "name": "crow-detector",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "auth:token": "ts-node -r tsconfig-paths/register scripts/generate-auth-token.ts",
    "build": "nest build",
    "commit": "cz",
    "coverage:open": "open ./coverage/lcov-report/index.html",
    "db:reset": "ts-node -r tsconfig-paths/register scripts/db-seed/reset.ts",
    "db:seed": "ts-node -r tsconfig-paths/register scripts/db-seed/seed.ts",
    "db:synchronize": "ts-node -r tsconfig-paths/register scripts/db-seed/synchronize.ts",
    "db:test": "ts-node -r tsconfig-paths/register scripts/db-seed/test-connection.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lambda:build": "cd lambda && npm run build",
    "lambda:clean": "cd lambda && npm run clean",
    "lambda:test:coverage": "cd lambda && npm run test:coverage",
    "lambda:lint": "cd lambda && npm run lint",
    "lambda:package": "cd lambda && npm run package",
    "lambda:test": "cd lambda && npm test",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "NODE_ENV=development concurrently \"npm run start:tunnel\" \"npm run start:watch\"",
    "start:prod": "node dist/main",
    "start:tunnel": "node scripts/tunnel.js -l 5432 -r 5432 -d crow-detector-db-dev.c32e8eak8kj0.us-west-2.rds.amazonaws.com",
    "start:watch": "nest start --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@aws-sdk/client-rekognition": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.872.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bunyan": "^1.8.11",
    "@types/crypto-js": "^4.2.2",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "commander": "^14.0.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
